  const TITLE = htmlWebpackPlugin.options.title !== 'Webpack App' ? htmlWebpackPlugin.options.title : 'MAP 預約顧問 - 人生藍圖畫起來 | 全球人壽 Transglobe';
  const COPYRIGHT = '2021 ©本網站版權屬 全球人壽保險股份有限公司所有';
  const fileName = /(?=\/).*(?=\.html$)/.exec(htmlWebpackPlugin.options.filename)[0];
  const reqFile = require('../views'+ fileName +'.ejs')({
    htmlWebpackPlugin : htmlWebpackPlugin
  });
