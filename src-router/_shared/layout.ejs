          <p>天主教輔仁大學關心您！</p>
        </noscript>
        <%- reqFile %>
      </main>
      <footer class="lFt" id="footer"></footer>
    </div>
    
    <%- require('container/mNSup.ejs')({
      cRight: COPYRIGHT,
      ieVersion: process.env.IE_VERSION
